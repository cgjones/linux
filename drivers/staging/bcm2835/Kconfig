config GE
	tristate "GE support"
	depends on BRCM_MODULES && BRCM_MODULES_PRIV && HANTRO_WRAP
	default N
	help
	  GE device driver support

config GE_WRAP
	tristate "Wrapper for GE Memory Allocater"
	default N
	---help---
		GE device driver support

config BRCM_V3D
	tristate "Broadcom OpenGL engine support"
	depends on BRCM_MODULES && BRCM_MODULES_PRIV
	default N
	config BRCM_V3D_OPT
		bool "Broadcom V3D Graphics Engine Optimized driver"
		depends on BRCM_V3D && BMEM
		default N
		help
		  Broadcom V3D Graphics Engine Optimized driver
	help
	  Broadcom OpenGL engine support
